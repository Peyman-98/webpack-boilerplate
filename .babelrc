{
  "env": {
    "test": {
      "plugins": [
        "transform-es2015-modules-commonjs",
        "dynamic-import-node"
      ],
      "presets": [
        "react",
        "stage-1"
      ]
    },
    "development": {
      "plugins": [
        "react-hot-loader/babel"
      ],
      "presets": [
        ["es2015", {"modules": false}],
        "stage-1",
        "react"
      ]
    },
    "production": {
      "plugins": [
        "transform-runtime"
      ],
      "presets": [
        ["es2015", {"modules": false}],
        "stage-1",
        "react"
      ]
    }
  }
}
